local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local UIS = game:GetService("UserInputService")

local LocalPlayer = Players.LocalPlayer
local PlayerGui = LocalPlayer:WaitForChild("PlayerGui")

do
    local prev = PlayerGui:FindFirstChild("FEARLESS_GUI")
    if prev then prev:Destroy() end
end

local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Name = "FEARLESS_GUI"
ScreenGui.ResetOnSpawn = false
ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
ScreenGui.Parent = PlayerGui

local MainFrame = Instance.new("Frame")
MainFrame.Size = UDim2.new(0, 360, 0, 260)
MainFrame.Position = UDim2.new(0.5, -180, 0.5, -130)
MainFrame.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
MainFrame.BorderSizePixel = 0
MainFrame.Parent = ScreenGui
Instance.new("UICorner", MainFrame).CornerRadius = UDim.new(0, 16)

spawn(function()
    while true do
        local r = (math.sin(tick())/2 + 0.5) * 80 + 60
        local g = (math.sin(tick()+2)/2 + 0.5) * 80 + 60
        local b = (math.sin(tick()+4)/2 + 0.5) * 80 + 60
        MainFrame.BackgroundColor3 = Color3.fromRGB(r,g,b)
        RunService.RenderStepped:Wait()
    end
end)

